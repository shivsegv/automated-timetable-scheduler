# Example environment file for frontend
# Copy this to .env.local for local development with remote backend

# API URL - Set this to your Render backend URL for production
# Leave empty or don't set for local development (uses proxy to localhost:8080)
REACT_APP_API_URL=

# For production deployment on Vercel, set this in Vercel dashboard:
# REACT_APP_API_URL=https://your-app.onrender.com
